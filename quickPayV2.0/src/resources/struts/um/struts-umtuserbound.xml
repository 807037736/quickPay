		<action name="delete" class="com.picc.um.web.UmTUserBoundAction" method="delete"/>
		
		<action name="update" class="com.picc.um.web.UmTUserBoundAction" method="update"/>
		
		<action name="queryWX" class="com.picc.um.web.UmTUserBoundAction" method="queryWX"/>
		
		<action name="queryCK" class="com.picc.um.web.UmTUserBoundAction" method="queryCK"/>
		
