		<action name="query" class="com.picc.um.web.UmTUserPowerAction" method="query"/>

		<action name="delete" class="com.picc.um.web.UmTUserPowerAction" method="delete"/>
		
		<action name="prepareQuery" class="com.picc.um.web.UmTUserPowerAction" method="prepareQuery">
			<result name="success">
				/pages/um/datapower/umtuserpower/UmTUserPowerQuery.jsp
			</result>
		</action>
		
		<action name="prepareUpdate" class="com.picc.um.web.UmTUserPowerAction" method="prepareUpdate">
