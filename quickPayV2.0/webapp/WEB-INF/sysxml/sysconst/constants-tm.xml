	<!-- 回执轮询线程池大小 -->
	<FetchReportThreadSize>100</FetchReportThreadSize>
	<!-- 回复轮询线程池大小 -->
	<FetchReplyThreadSize>100</FetchReplyThreadSize>
	<!-- 轮询分页结果大小，默认为10000 -->
	<PageResultCount>10000</PageResultCount>
	<!-- 查询回执时间窗口大小，天数 -->
	<getReportDateLength>3</getReportDateLength>
	<!-- 查询回复时间窗口大小，天数 -->
	<getReplyDateLength>5</getReplyDateLength>
	<!-- 查询中间表时间窗口大小，天数 -->
	<getMidToSendDateLength>3</getMidToSendDateLength>
	<!-- 短信回复回调流程方式  1-内部调用  2 WebService -->
	<SMSReplyCallBackMethod>1</SMSReplyCallBackMethod>
	<!-- SendMessage历史记录N天转储 -->
	<TransSendHisIntervalDays>100</TransSendHisIntervalDays>
	
	<!-- Flex历史记录N天转储 -->
	<TransFlexHisIntervalDays>10</TransFlexHisIntervalDays>

	<!-- 转储使用存储过程( 0-java方式  1-表示调用存储过程)-->
